	   $.post("/cooperation/city",function(data){
		   if(data != ""){
			   var option = "";
			   for(var i =0;i<data.length;i++){
				   option +="<option value="+data[i].id+">"+data[i].city_name+"</option>";
			   }
			   $("#provice option").remove();
			   $("#provice").append("<option value='0'>请选择省份</option>");
			   $("#provice").append(option);
		   }
	   },"json");
   });
   
   /**查询服务商**/
   function query(){
	   layer.alert("无此服务商。");
   }
   
   /**省份改变事件**/
   function proviceChange(obj){
	   if(obj.value != ""){
		   $.post("/cooperation/city",{cityid : obj.value},function(data){
			   if(data != ""){
				   var option = "";
				   for(var i =0;i<data.length;i++){
					   option +="<option value="+data[i].id+">"+data[i].city_name+"</option>";
				   }
				   $("#city option").remove();
				   $("#city").append(option);
			   }
		   },"json");
	   }
   }
   
   /**城市改变事件**/
   function cityChange(obj){
	   if($(obj).val() != ""){
		   $("#cityPrompt").text("");
	   }
   }
   
   /**发送短信**/
   function sendMsg(){
	   $.post("/servlet/CooperationServlet",{opt : 4},function(data){
		  
	   },"json");
   }
   
   
   /**提交事件**/
   function submit(){
	   if(!isEmpty()){
		   var params = {};
		   $(".inform").eq(0).find("input").each(function(){
			   params[$(this).attr("name")] = $(this).val(); 
		   });
		   params["cooAddress"] = "中华人民共和国"+$("#provice").find("option:selected").text()+$("#city").find("option:selected").text();
		   params["opt"] = 3;
		   layer.load();
		   $.post("/cooperation/sumbit",params,function(data){
	   			if(data.result){
	   				layer.alert(data.message);
	   				setTimeout("reload()",5000);
	   			}else{
	   				layer.alert("提交失败，系统繁忙！");
	   			}
	   		},"json");
	   }
   }
   
   function reload(){
	   location.reload();
   }
   
   /**非空判断**/
   function isEmpty(){
	   var flag = false;
	   $(".inform").eq(0).find("tr").each(function(){
		   if($(this).find("td").eq(0).text() == "*"){
			   if($(this).find("input").eq(0).val() == ""){
				   $(this).find("td").eq($(this).find("td").length - 1).addClass("errorTextColor");
				   $(this).find("td").eq($(this).find("td").length - 1).text("不能为空！");
				   flag = true;
			   }
		   }
	   });
	   if($("#provice").val() <= "0" || $("#city").val() <= "0"){
		   $("#cityPrompt").text("不能为空！");
		   flag = true;
	   }
	   return flag;
   }
</script>
