<script src="../js/jquery-2.2.2.min.js"></script>
<script>
  var _wxstFloor = function () {

  }
  _wxstFloor.prototype.flag420 = null
  _wxstFloor.prototype.flag478 = null
  _wxstFloor.prototype.wxstFloor4LoadFlag = null
  _wxstFloor.prototype.maskFlag = null
  _wxstFloor.prototype.wxstFloor420 = function () {
    var m = 0
    var self = this
    self.flag420 = setInterval(function () {
      if (m < 20) {
        m += 1
        $('.wxstFloor420').html(m)
      }else{
        clearInterval(self.flag420)
      }
    }, 70)
  }
  _wxstFloor.prototype.wxstFloor478 = function () {
    var m = 0
    var self = this
    self.flag478 = setInterval(function () {
      if (m < 78) {
        m += 1
        $('.wxstFloor478').html(m)
      } else {
        clearInterval(self.flag478)
      }
    }, 15)
  }
  _wxstFloor.prototype.wxstFloor4Load = function () {
    var left = -178
    var self = this
    self.wxstFloor4LoadFlag = setInterval(function () {
      left += 1
      if (left <= 0) {
        $('#wxstFloor4Load').css({'left': left + 'px'})
      } else {
        clearInterval(self.wxstFloor4LoadFlag)
      }
    }, 6)
  }
  _wxstFloor.prototype.wxstFloor3Mask = function(){
    var num = 327;
    var self = this;
    self.maskFlag = setInterval(function(){
      if(num>0){
        num-=3;
        $('#wxstFloor3Mask').css({"width":num+"px"})
      }else{
        clearInterval(self.maskFlag)
      }
    },10)
  }
  var flag = 1
  var flag2 = 1
  var wxstFloor = new _wxstFloor()
  var scrollTop1 = document.documentElement.scrollTop || document.body.scrollTop
  console.log(scrollTop1)
  if(scrollTop1 >= 600){
    flag+=1
    wxstFloor.wxstFloor420()
    wxstFloor.wxstFloor478()
    wxstFloor.wxstFloor4Load()
    wxstFloor.wxstFloor3Mask()
  }
  var innerHeight = window.innerHeight
  if(innerHeight >= 960){
    wxstFloor.wxstFloor3Mask()
  }
  window.onscroll = function (e) {
    var scrollTop = document.documentElement.scrollTop || document.body.scrollTop
    var innerHeight = window.innerHeight
    if (scrollTop > 100 && innerHeight < 750) {
      flag += 1
      if (flag === 2) {
        wxstFloor.wxstFloor3Mask()
      }
    }
    if (scrollTop > 300 && scrollTop < 450) {
      flag2 += 1
      if (flag2 === 2) {
        wxstFloor.wxstFloor420()
        wxstFloor.wxstFloor478()
        wxstFloor.wxstFloor4Load()
      }
    }
  }
  $(function () {
    $('#wxstFloor2Item').find('li').on('mouseover', function () {
      $(this).addClass('hover').siblings('li').removeClass('hover')
    })
  })
</script>
