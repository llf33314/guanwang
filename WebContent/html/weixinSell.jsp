	var imgint;
	var imgsrc;
	$('.wx_bottom li ').hover(function() {

		imgint = $(this).find('img').attr('src').toString();
		imgsrc = imgint.substring(0, imgint.length - 4);
		$(this).find('img').attr('src', imgsrc + 'blue.png');
		$(this).find('img').siblings().addClass('wxblue')
	}, function() {
		$(this).find('img').removeClass('play');
		$(this).find('img').attr('src', imgsrc + '.png');
		$(this).find('img').siblings().removeClass('wxblue')
	});
	
</script>
<script type="text/javascript">
    
