            setTimeout(function(){
              $('#cxbFnLeftBox').addClass('leave-left-img').removeClass('show-left-img')
            },1300)
          },
          // 显示控制面板
          showControl:function(){
             setTimeout(function(){
               $('#cxbFnControl').removeClass('hide')
             },5000)
          },
          // 定时切换菜单栏
          setInterValcxbFnArrowAuto:function(){
            var self = this;
            self.CXBINTER =  setInterval(function(){
              self.CXBMUN+=1
              if(self.CXBMUN >= $('#cxbFnLeftArrowList').find('li').length){
                self.CXBMUN = 0
                self.cxbFnLeftRightArrow(1)
              }
              if(self.CXBMUN >= 4){
                self.cxbFnLeftRightArrow(2)
              }
              $('#cxbFnLeftArrowList').find('li').eq(self.CXBMUN).addClass('set-inter').siblings('li').removeClass('set-inter')
              self.switchAnimate(self.CXBMUN)
            },10000)
          },
          // 暂停、开始
          stopOrRun:function(t){
            this.flag = !this.flag;
            console.log('this.flag'+this.flag)
            if(CxbFnAnimate.num >=4){
              CxbFnAnimate.cxbFnLeftRightArrow(2)
            }else{
              CxbFnAnimate.cxbFnLeftRightArrow(1)
            }
            if(!this.flag){
              window.speed = 0.1
              drawFrame()
              window.cancelAnimationFrame(stop);
              console.log('CxbFnAnimate.num+++'+CxbFnAnimate.num)
              $('#cxbFnLeftArrowList').find('li').eq(CxbFnAnimate.num).removeClass('set-inter').siblings('li').removeClass('set-inter')
              $('#cxbFnLeftArrowList').find('li').eq(CxbFnAnimate.num).css({"border":"1px solid #1b40b3"})
            }else{
              $('#cxbFnLeftArrowList').find('li').css({"border":"1px solid #fff"})
              $('#cxbFnLeftArrowList').find('li').eq(CxbFnAnimate.num).addClass('set-inter').siblings('li').removeClass('set-inter')
            }
            $(t).siblings('.fn-state-btn').toggleClass('state') //按钮图片
          },
          // 切换内容
          switchAnimate:function(num){
            var self = this;
            num = num > 4 ? 0:(num+1)
            setTimeout(function(){
              console.log('即将运行的index**'+num)
              self.leaveAnimateClass()
              setTimeout(function(){
                self.addAnimateClass(num);
              },1500)
            },5000)
          },
          // -------------------------------
          // 显示进度条 10s
          showLoad:function(num){
            if(!this.flag){
             return
            }
            $('#cxbFnLeftArrowList').find('li').eq(num).addClass('set-inter').siblings('li').removeClass('set-inter')
          },
          // 显示，隐藏控制面板
          showOrHideControl:function(state){
            if(!state)  $('#cxbFnControl').addClass('hide')
            else  $('#cxbFnControl').removeClass('hide')
          },
          // 首屏加载动画
          cxbFnFirstAnimate:function(state){
            console.log(555)
            if(state) $('#cxbFnFirstAnimate').addClass('begin-first-animate')
            else $('#cxbFnFirstAnimate').hide()
          },
          //  去除动画
          deleteAnimate:function(){
            this.showOrHideControl(false)
            $('#showRight0').removeClass('show-right1')
            $('#cxbFnLeftBox').removeClass('show-left-img')
            $('body').find('.cxb-fn-left').hide()
            $('#cxbFnLeftContent').find('.cxb-fn-left-content').removeClass('start-animate')
            // $('#showBoxRightz').find('.show-box-child').removeClass('show-right0')
            $('#showBoxRightz').find('.show-box-child').removeClass('show-right1')
            window.cancelAnimationFrame(stop);
            window.speed = 0.1
          },
          contentAnimate:function(state,index,num){
            if(!state || index == this.index) return
            var self = this;
            console.log('self.flag++'+self.flag)
            var num = num == null ? self.num:num;
            self.index = index;
            self.num = num;
            $('#cxbFnLeftArrowList').find('li').css({"border":"1px solid #fff"})
            if(!self.flag){
              $('#cxbFnLeftArrowList').find('li').eq(index).css({"border":"1px solid #1b40b3"})
              $('#cxbFnLeftArrowList').find('li').eq(index).removeClass('set-inter')
            }else{
              $('#cxbFnLeftArrowList').find('li').eq(index).css({"border":"1px solid #fff"})
            }
            self.deleteAnimate();
            // 中间加载
            $('#showRight'+num).addClass('show-right1')
            // 左侧大图
            $('#cxbFnLeftBox').addClass('show-left-img').find('.cxb-fn-left').eq(num).show();
            // 左侧内容
            setTimeout(function(){
              $('#cxbFnLeftContent').find('.cxb-fn-left-content').eq(num).addClass('start-animate').siblings('.cxb-fn-left-content').removeClass('start-animate')
            },1700)
            setTimeout(function(){
              self.showOrHideControl(true)
              self.showLoad(num)
              drawFrame()
            },4000)
          },
        }
        $(function(){
          //数字变化
          $('.counter').countUp({
            'time': 1000,
            'delay': 10
          });
          CxbFnAnimate.cxbFnFirstAnimate(true);
          setTimeout(function(){
            $('#bgBoxTop').css({"opacity":"1"})
            CxbFnAnimate.contentAnimate(true,0,0)
          },3500)
        })


</script>
</body>
</html>