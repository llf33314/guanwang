	<script type="text/javascript">
		function showImg() {
			document.getElementById("wxImg").style.display = 'block';
		}
		function hideImg() {
			document.getElementById("wxImg").style.display = 'none';
		}
		function manageChange() {
			$('.manage-box .region').eq(0).fadeIn();
			$('.manage .btn div').each(function (i, dom) {
				$(dom).click(function () {
					if ($(this).hasClass('active')) {
						return false;
					}
					$('.manage .btn div').removeClass('active')
					$(this).addClass('active');
					$('.manage-box .region').css("display", "none");
					$('.manage-box .region').eq(i).css("display", "block");
					var wow = new WOW({
						offset: 250,
					});
					wow.init();
				})
			})
		}
		$(function () {
			manageChange()
			var wow = new WOW({

				offset: 250,
			});
			wow.init();
			var wow1 = new WOW({
				boxClass: 'wowone',
				offset: 10,
			});
			wow1.init();
		})
