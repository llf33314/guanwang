		function manageChange() {
			$('.manage-box .region').eq(1).fadeIn();
			$('.manage .btn div').each(function (i, dom) {
				$(dom).click(function () {
					if ($(this).hasClass('active')) {
						return false;
					}
					$('.manage .btn div').removeClass('active')
					$(this).addClass('active');
					$('.manage-box .region').fadeOut();
					$('.manage-box .region').eq(i).fadeIn();
				})
			})
		}
		$(function () {
			manageChange();
			$('.Case .num span').countUp();
		})
