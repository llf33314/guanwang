	<script type="text/javascript">
		function bannerResize() {
			$(window).resize(function () {
				var h = $(window).height() >= 766 ? $(window).height() : 766;
				if (h == 766) {
					$(".home-banner>div").css({ 'top': "0px" });
					$(".home-banner .banner-title").css({ 'margin-top': "40px" });
					$(".home-banner .video-box").css({ 'margin': "1% 0" });
				} else {
					$(".home-banner>div").css({ 'top': "initial" });
					$(".home-banner .banner-title").css({ 'margin-top': "0px" });
					$(".home-banner .video-box").css({ 'margin': "2% 0" });
				}
				$(".home-banner").height((h - 80) + "px");
			}).trigger('resize');
		}
		$(function () {
			bannerResize();
			var wow = new WOW({
				offset: 10,
			});
			wow.init();
		})
	</script>
